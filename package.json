{
  "name": "sequencer",
  "version": "1.0.0",
  "description": "Simple JS step sequencer",
  "main": "webpack.config.js",
  "scripts": {
    "start": "webpack-dev-server",
    "prod": "npm run clean && NODE_ENV=production webpack -p",
    "clean": "rimraf ./dist/*",
    "deploy": "npm run prod && gh-pages -d dist"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "impliedStrict": true,
        "jsx": false
      }
    },
    "extends": "eslint:recommended",
    "rules": {
      "object-shorthand": "warn",
      "prefer-arrow-callback": "warn"
    },
    "env": {
      "es6": true,
      "node": true,
      "jquery": true
    }
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afuh/sequencer.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/afuh/sequencer/issues"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "github-pages": "^3.0.2",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "jquery": "^3.2.1"
  }
}
